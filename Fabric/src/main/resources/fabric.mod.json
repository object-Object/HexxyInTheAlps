{
  "schemaVersion": 1,
  "id": "hexxyinthealps",
  "version": "${version}",
  "name": "Hexxy Searches For A Lost Cat In The Alps",
  "description": "A per-player server-side profiler and lag detector for Hex Casting.",
  "authors": [
    "object-Object"
  ],
  "contact": {
    "homepage": "https://github.com/object-Object/HexxyInTheAlps",
    "sources": "https://github.com/object-Object/HexxyInTheAlps"
  },
  "license": "MIT",
  "icon": "assets/hexxyinthealps/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "gay.object.hexxyinthealps.fabric.FabricHexxyInTheAlps"
    }],
    "client": [{
      "adapter": "kotlin",
      "value": "gay.object.hexxyinthealps.fabric.FabricHexxyInTheAlpsClient"
    }],
    "fabric-datagen": [{
      "adapter": "kotlin",
      "value": "gay.object.hexxyinthealps.fabric.datagen.FabricHexxyInTheAlpsDatagen"
    }],
    "modmenu": [{
      "adapter": "kotlin",
      "value": "gay.object.hexxyinthealps.fabric.FabricHexxyInTheAlpsModMenu"
